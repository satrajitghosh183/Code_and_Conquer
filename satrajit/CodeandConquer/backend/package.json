{
  "name": "code-and-conquer-backend",
  "version": "1.0.0",
  "description": "Code and Conquer Backend - Competitive Coding Platform with Tower Defense Game",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'No build step required for Node.js'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "lint": "echo 'Add ESLint configuration for linting'",
    "test": "echo 'Add test runner configuration'",
    "sync-tags": "node src/scripts/syncProblemTags.js",
    "sync-problems": "node scripts/databaseOperations.js sync-problems",
    "list-tables": "node scripts/databaseOperations.js list-tables",
    "generate-test-cases": "node scripts/generateTestCases.js",
    "upload-test-cases": "node scripts/manageTestCases.js upload",
    "fix-test-format": "node scripts/manageTestCases.js fix-format",
    "fix-design-tests": "node scripts/manageTestCases.js fix-design",
    "health-check": "curl -s http://localhost:5000/api/health | node -e \"process.stdin.on('data', d => { const h = JSON.parse(d); console.log('Status:', h.status); process.exit(h.status === 'ok' ? 0 : 1); })\""
  },
  "keywords": [
    "leetcode",
    "coding-challenges",
    "tower-defense",
    "competitive-programming",
    "supabase"
  ],
  "author": "Code and Conquer Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "fs-extra": "^11.3.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.2",
    "mathjs": "^15.0.0",
    "multer": "^1.4.5-lts.1",
    "regression": "^2.0.1",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.1",
    "tar-stream": "^3.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "nodemon": "^3.1.10"
  }
}
